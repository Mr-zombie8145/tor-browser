+    /// (Virtual) Bus (local apic timer) frequency in kHz.
+    pub fn apic_frequency(&self) -> Option<u32> {
+        // # EBX: (Virtual) Bus (local apic timer) frequency in kHz.
+        if self.res.eax >= 0x40000010 {
+            let virt_tinfo = cpuid!(0x40000010, 0);
+            Some(virt_tinfo.ebx)
+        } else {
+            None
+        }
+    }
