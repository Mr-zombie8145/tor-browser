+    // Level type.
+    pub fn level_type(&self) -> TopologyType {
+        match get_bits(self.ecx, 8, 15) {
+            0 => TopologyType::Invalid,
+            1 => TopologyType::SMT,
+            2 => TopologyType::Core,
+            _ => unreachable!(),
+        }
+    }
