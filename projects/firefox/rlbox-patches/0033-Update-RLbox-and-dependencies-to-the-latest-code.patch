+        /// LAHF/SAHF available in 64-bit mode.
+        const LAHF_SAHF = 1 << 0;
+        /// Bit 05: LZCNT
+        const LZCNT = 1 << 5;
+        /// Bit 08: PREFETCHW
+        const PREFETCHW = 1 << 8;
+    }
